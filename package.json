{
  "name": "lynx-examples",
  "version": "0.0.0",
  "private": true,
  "description": "The examples for Lynx",
  "license": "Apache-2.0",
  "author": "Lynx Authors",
  "scripts": {
    "build": "turbo run build",
    "prepare": "husky",
    "release": "npm run build && pnpm changeset publish"
  },
  "nano-staged": {
    "*.{css,scss,js,jsx,ts,tsx,mjs,cjs,json,jsonc,md}": [
      "dprint fmt --allow-no-files --"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@pnpm/meta-updater": "^2.0.6",
    "@rsbuild/plugin-type-check": "1.3.0",
    "dprint": "^0.50.2",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "sort-package-json": "^3.4.0",
    "turbo": "^2.5.8"
  },
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">=22",
    "pnpm": "10.18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "dprint"
    ],
    "overrides": {
      "@lynx-js/types": "catalog:"
    },
    "ignoredBuiltDependencies": [
      "core-js"
    ]
  }
}
